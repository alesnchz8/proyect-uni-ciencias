{
  "expo": {
    "name": "UNI-Ciencias",
    "slug": "uni-ciencias",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/buho.png",
    "splash": {
      "image": "./src/assets/images/iconsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF5029"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/buho.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
